{
    "name": "QA Helper",
    "description": "Assists the QA engineer in getting started with the ticket",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "action": {
        "default_popup":"popup.html"
    },
    "permissions": [
        "tabs",
        "tabGroups",
        "scripting", 
        "activeTab"
    ],
    "background":{}
}